
public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Fruta banana = new Fruta();
		banana.caloriasPorGrama = 10;
		banana.gramas = 100;
		
		Fruta laranja = new Fruta();
		laranja.caloriasPorGrama = 1;
		laranja.gramas = 200;
		
		System.out.println(banana.totalCalorias());
		
		System.out.println(laranja.totalCalorias());
	}

}
